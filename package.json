{
  "name": "ornament-helpers",
  "version": "0.2.1",
  "description": "Helpers",
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "NODE_ENV=production babel src --out-dir lib --copy-files",
    "start": "npm run build -- --watch",
    "test": "npm run build && mocha \"test/**/*.js\"",
    "lint": "npm run build && eslint --ext .es,.js --ignore-path .eslintignore --format codeframe ./"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OrnamentStudio/helpers.git"
  },
  "author": "Abylay Keldibek <atomio.ak@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/OrnamentStudio/helpers/issues"
  },
  "homepage": "https://github.com/OrnamentStudio/helpers#readme",
  "engines": {
    "node": "^8.x.x"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-stage-3": "6.24.1",
    "chai": "^4.2.0",
    "eslint": "^5.7.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "mocha": "^5.2.0"
  }
}
